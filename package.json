{
  "name": "delete-old-github-package-versions",
  "version": "1.0.0",
  "description": "GitHub Action to delete GitHub package versions older than a specified age",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest --passWithNoTests",
    "all": "npm run clean && npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abrkn/delete-old-github-package-versions.git"
  },
  "keywords": [
    "actions",
    "delete",
    "github",
    "packages",
    "docker"
  ],
  "author": "Andreas Brekken <andreas@brekken.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.3",
    "@actions/github": "2.2.0",
    "@octokit/graphql": "4.4.0",
    "lodash": "4.17.19",
    "npm-run-all": "4.1.5",
    "parse-duration": "0.4.3"
  },
  "devDependencies": {
    "@types/jest": "24.9.1",
    "@types/lodash": "4.14.150",
    "@types/node": "12.12.35",
    "@typescript-eslint/parser": "2.27.0",
    "@zeit/ncc": "^0.20.5",
    "dotenv": "8.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^29.6.1",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "24.3.0",
    "ts-node": "8.10.1",
    "typescript": "3.8.3"
  }
}
